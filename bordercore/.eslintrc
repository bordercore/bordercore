{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
      "vue/script-indent": [
        "error",
        4,
        {
          "baseIndent": 1
        }
      ],
      "indent": "off",
      "max-len": "off",
      "quotes": [2, "double"]
    }
}
